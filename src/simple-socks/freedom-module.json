{
  "name": "Simple SOCKS",
  "description": "Single-page SOCKS server with fake signalling channel.",
  "app": {
    "script": [
      "freedom-module.static.js"
    ]
  },
  "dependencies": {
    "loggingcontroller": {
      "url": "../loggingprovider/freedom-module.json",
      "api": "loggingcontroller"
    },
    "logginglistener": {
      "url": "../loggingprovider/freedom-module.json",
      "api": "logginglistener"
    },
    "churnPipe": {
      "url": "../churn-pipe/freedom-module.json",
      "api": "churnPipe"
    }
  },
  "permissions": [
    "core.tcpsocket",
    "core.udpsocket",
    "core.rtcdatachannel",
    "core.rtcpeerconnection"
  ]
}
