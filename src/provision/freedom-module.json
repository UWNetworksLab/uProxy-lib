{
  "name": "provision",
  "description": "Provisions VMs on Digital Ocean",
  "app": {
    "script": [
      "/forge-min/forge.min.js",
      "freedom-module.static.js"
    ]
  },
  "provides": [ "provision" ],
  "default": "provision",
  "api": {
    "provision": {
      "constructor": { "value": [ ] },
      "start": {
        "type": "method",
        "value": [ "string" ],
        "ret": {
          "oauth": {
            "access_token": "string",
            "expires_in": "string",
            "state": "string",
            "token_type": "string"
          },
          "ssh": {
            "public": "string",
            "private": "string"
          }
        },
        "err": {
          "errcode": "string",
          "message": "string"
        } 
      },
      "status": {
        "type": "event",
        "value": {
          "code": "string",
          "message": "string"
        }
      }
    }
  },
  "permissions": [
    "core.xhr",
    "core.oauth",
    "core.storage"
  ]
}
