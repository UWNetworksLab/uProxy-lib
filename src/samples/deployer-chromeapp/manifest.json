{
  "manifest_version": 2,
  "name": "deployer",
  "description": "Provision a uProxy cloud instance.",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "identity",
    "storage",
    "<all_urls>"
  ],
  "sockets": {
    "tcp": {
      "connect" : ""
    },
    "tcpServer": {
      "listen": ""
    },
    "udp": {
      "bind": "*",
      "send": "*"
    }
  },
  "key": ""
}
